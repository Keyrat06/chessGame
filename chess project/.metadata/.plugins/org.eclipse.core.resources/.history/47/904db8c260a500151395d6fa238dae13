package board;

import static org.junit.Assert.*;

import org.junit.Test;

public class BoardTest {

	@Test
	public void test() {
		Board newBoard = new Board();
		Integer[] location = new Integer[2];
		location[0] = 1;
		location[1] = 0;
		System.out.println(newBoard.possibleMovesForPiece(location));
		assertTrue(true);
	}

}
